    <!-- metalex:naming attribute (value: an IRI) must be used to signal conformance to a naming convention -->
    <!-- xml:base attribute is an identifier of the manifestation of the legislation -->
    <!-- xml:lang attribute is an identifier of the language of the legislation -->
    
    <!-- 
    <title>З А К О Н О ЗАШТИТИ ПОДАТАКА О ЛИЧНОСТИ</title>
    -->
    
    <!-- Metadate in RDFa format will not "validate" against OWL schema because it is not complete. Use metadata in Protege instead. -->
    <metadata id="md" metalex:type="mcontainer">
        <!-- all attribute names must be prefixed -->
        
        <!-- The RDF interpretation of a name MUST assign an IRI reference to the work, expression, and manifestation, and link these 
            using the properties metalex-owl:embodies and metalex-owl:realizes p. 25 -->
        
        <!-- 
            IRI format compatibile with urn:lex URN namespace [TODO: Write textual specification of the format]
            
            "urn:lex
            :"<jurisdiction-code>[";"<jurisdiction-unit>]*"
            :"<authority>":"<measure>":"<details>[":"<annex>]*
            @<expression>::="@"<version>[":"<language>]?
            $<format>[";"<specification>"]*":"<editor>[";"<specification>]*[":"<component>[";"<specification>]*]?
            [":"<feature>[";"<specification>]*]?
            
            urn:lex:rs:narodna-skupstina:zakon:2008-10-23;lex-1@original:sr$text-xml;cen-metalex:sluzbeni-glasnik
            urn:lex:rs:sluzbeni-glasnik:zakon:2008-10-27;2375@original:sr$text-xml;cen-metalex:sluzbeni-glasnik
            
            "
        -->
        
        <!-- Expression realizes Work -->
        <meta id="m1" metalex:about="urn:lex:rs:narodna-skupstina:ustav:2006-11-08;lex-1@original:sr"
            metalex:rel="metalex-owl:realizes"
            href="urn:lex:rs:narodna-skupstina:ustav:2006-11-08;lex-1" metalex:type="meta"/>
        
        <!-- Manifestation emodies Expression -->
        <meta id="m2"
            metalex:about="urn:lex:rs:narodna-skupstina:ustav:2006-11-08;lex-1@original:sr$text-xml;cen-metalex:sluzbeni-glasnik"
            metalex:rel="metalex-owl:embodies"
            href="urn:lex:rs:narodna-skupstina:ustav:2006-11-08;lex-1@original:sr" metalex:type="meta"/>
        
        <!-- Manifestation must be examplified by at least one Item -->
        <meta id="m3"
            metalex:about="urn:lex:rs:narodna-skupstina:ustav:2006-11-08;lex-1@original:sr$text-xml;cen-metalex:sluzbeni-glasnik"
            metalex:rel="metalex-owl:exemplifiedBy" href="http://informatika.ftn.uns.ac.rs/legal/metalex/constitution.xml"
            metalex:type="meta"/>
        
        <!-- FRBR Work Level Metadata -->
        
        <!-- country -->
        <meta id="m5"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#ConstitutionInentifier"
