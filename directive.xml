            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#ConstitutionInentifier"
            metalex:rel=" http://www.metalex.eu/metalex/2008-05-02#identifierComponent"
            metalex:href="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#Lex-1"
            metalex:type="meta" />
        
        <meta id="m12"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#Lex-1"
            metalex:property="http://www.metalex.eu/metalex/2008-05-02#issueID"
            metalex:content="lex-1"
            metalex:type="meta" />
         
        <!-- FRBR expression level metadata -->
        
        <!-- languages -->
        <meta id="m13"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#ConstitutionInentifier"
            metalex:rel="http://www.metalex.eu/metalex/2008-05-02#identifierComponent"
            metalex:href="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#SR"
            metalex:type="meta" />
        
        <meta id="m14"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#SR"
            metalex:property="http://www.metalex.eu/metalex/2008-05-02#languageCode"
            metalex:content="sr"
            metalex:type="meta" />
        
        <!-- validity dates -->
        <!-- authoring information 
             Expression resultOf LegislativeModification agent
        -->
        
        <!-- content-specification date -->
        <meta id="m15"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#ConstitutionInentifier"
            metalex:rel="http://www.metalex.eu/metalex/2008-05-02#identifierComponent"
            metalex:href="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#PrivacyLawExpressionCreationDate"
            metalex:type="meta" />
        
        <meta id="m16"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#PrivacyLawExpressionCreationDate"
            metalex:property="http://www.metalex.eu/metalex/2008-05-02#xsdDate"
            metalex:content="2006-11-08"
            metalex:datatype="date"
            metalex:type="meta" />   
        
        <!-- FRBR manifestation level metadata -->
        
        <!-- format -->
        <meta id="m17"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#ConstitutionInentifier"
            metalex:rel="http://www.metalex.eu/metalex/2008-05-02#identifierComponent"
            metalex:href="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#CENMetaLex"
            metalex:type="meta" />
        
        <meta id="m18"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#CENMetaLex"
            metalex:property="http://www.metalex.eu/metalex/2008-05-02#manifestationTypeID"
            metalex:content="text-xml;cen-metalex"
            metalex:type="meta" />
        
        <!-- markup authoring information [obligatory, TODO] 
             Manifestation resultOf LegislativePublication agent -->
        <!-- markup specific date 
            Manifestation resultOf LegislativePublication date -->
        <!-- markup-related information (multiple versions, etc.) -->    
    </metadata>
    
