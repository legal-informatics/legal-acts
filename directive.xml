            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#ConstitutionInentifier"
            metalex:rel="http://www.metalex.eu/metalex/2008-05-02#identifierComponent"
            metalex:href="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#PrivacyLawWorkCreationDate"
            metalex:type="meta" />
        
        <meta id="m10"
            metalex:about="http://informatika.ftn.uns.ac.rs/legal/metalex/1.0#PrivacyLawWorkCreationDate"
